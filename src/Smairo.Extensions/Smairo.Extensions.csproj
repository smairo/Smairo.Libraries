<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Version>3.0.0</Version>
    <OutputType>Library</OutputType>
    <IsPackable>true</IsPackable>
    <Authors>Miro Holopainen</Authors>
    <Description>Contains general extensions for basic types</Description>
    <PackageProjectUrl>https://github.com/smairo/Smairo.Libraries/tree/master/src/Smairo.Extensions</PackageProjectUrl>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
    <RepositoryUrl>https://github.com/smairo/Smairo.Libraries</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NodaTime" Version="3.0.5" />
  </ItemGroup>

</Project>
