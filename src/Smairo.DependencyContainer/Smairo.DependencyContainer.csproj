<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <OutputType>Library</OutputType>
    <Version>2.0.0</Version>
    <Authors>Miro Holopainen</Authors>
    <Description>Contains container builder and module, that allows you to use dependency injection in pretty much anywhere</Description>
    <PackageProjectUrl>https://github.com/smairo/Smairo.Libraries/blob/master/src/Smairo.DependencyContainer/README.md</PackageProjectUrl>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
    <RepositoryUrl>https://github.com/smairo/Smairo.Libraries</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="3.0.0" />    
  </ItemGroup>

</Project>